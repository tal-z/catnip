{

  "manifest_version": 2,
  "name": "Catnip",
  "version": "1.0",
  "description": "When enabled, replaces all images on a page with images of cats.",
  "permissions": ["<all_urls>", "tabs"],

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "48": "icons/cat-48.png"
    }
  }

}
